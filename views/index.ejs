<% include partials/header %>


<h1>Index Page</h1>



<% blogsIndexPage.forEach(function(blog){ %>

<div>
    <h2> <%= blog.title %> </h2>
        <img src = <%= blog.image %> >
        <p>  <%= blog.created.toDateString() %> </p>
        <p>  <%= blog.body.substring(0,100) %>... </p>
        <a href ="/blogs/<%= blog._id%>">Read More </a>
</div>

<% }); %>
<% include partials/footer %>